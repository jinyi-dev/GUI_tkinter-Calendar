from tkinter import *

import calendar


text = calendar.calendar(2022)

# creating tkinter window
root = Tk()
root.geometry("500x600")
root.title('2022 Calendar')
lable1=Label(root, text ='CALENDAR', bg ='dark grey', font = ('times', 28, 'bold'))
lable1.grid (row =1, column =1)
root.config(background ='white')


root.mainloop()
